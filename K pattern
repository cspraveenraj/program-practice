n=int(input())
m=int(input())
for i in range(n):
    for j in range(m):
        if (j==0 or (i==0 and j==3) or (i==1 and j==2) or (i==2 and j==1)):
            print("#",end=" ")
        elif (i==a and j==b):
            print("#",end=" ")
            a=a+1
            b=b+1
        else:
            print(end=" ")
    print()      
