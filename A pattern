n=int(input())
m=int(input())
for i in range(n):
    for j in range(m):
        if((j==0 or j==4 or i==2) and i!=0):
            print("$",end=" ")
        elif ((i==0 and j==1) or (i==0 and j==2) or (i==0 and j==3)):
            print("$",end=" ")
        else:
            print(end="  ")
    print()
