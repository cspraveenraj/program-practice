n=int(input())
m=int(input())
for i in range(n):
    for j in range(m):
        if (j==0 or i==0 or i==3 or i==6 or j==3):
            print("@ ",end=" ")
        elif(i + j==3 or i + j==6 or i+j==2 or i+j==4 or i+j==5 or i+j==7):
            print(".",end="  ")
        else:
            print(end="   ")
    print()  
