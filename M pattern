n=int(input())
m=int(input())
for i in range(n):
    for j in range(m):
        if(j==0 or j==6):
            print("@",end=" ")
        elif((i==1 and j==1) or (i==2 and j==2) or (i==3 and j==3)):
            print("@",end=" ")
        elif(i==a and j==b):
            print("@",end=" ")
            a+=1
            b-=1
        else:
            print(end="  ")
    print()
